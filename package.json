{
  "name": "premium-banking-monorepo",
  "private": true,
  "workspaces": [
    "apps/web",
    "apps/admin",
    "apps/api"
  ],
  "scripts": {
    "dev": "npm --prefix apps/web run dev",
    "build": "npm run build -w apps/api && npm run build -w apps/web",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "format": "prettier --write .",
    "typecheck": "tsc -b",
    "test": "echo 'No tests yet' && exit 0",
    "db:setup": "yarn workspace premium-banking-api db:setup"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "packageManager": "npm@9.8.1",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/hybeprojects/dashboard",
  "author": "BLCK-DEV",
  "license": "MIT"
}
