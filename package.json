{
  "name": "premium-banking-monorepo",
  "private": true,
  "workspaces": [
    "apps/web"
  ],
  "scripts": {
    "dev": "npm --prefix apps/web run dev",
    "build": "npm --prefix apps/web run build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc -b",
    "test": "echo 'No tests yet' && exit 0",
    "test:e2e": "npx playwright test",
    "ci": "npm ci && npm run lint || true && npm run typecheck && npm run build",
    "start:prod": "npm --prefix apps/web run build && npm --prefix apps/web run start",
    "semgrep": "semgrep --config .semgrep.yml || true"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@next/eslint-plugin-next": "^16.0.0",
    "@playwright/test": "^1.56.1",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "npm@11.6.2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/hybeprojects/dashboard",
  "author": "BLCK-DEV",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.15",
    "axios": "^1.12.2",
    "framer-motion": "^10.12.16"
  }
}
